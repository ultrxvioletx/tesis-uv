{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.typ$",
                "cmd": "typst compile main.typ",
            }
        ]
    },
    "editor.tabSize": 2,
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "/usr/sbin/bash",
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "/bin/zsh"
        }
    },
    "terminal.integrated.defaultProfile.linux": "zsh"
}